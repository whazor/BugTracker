<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="BugTracker.App">
	<Application.Resources>
		<!-- Resources scoped at the Application level should be defined here. -->
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Microsoft.Expression.Prototyping.SketchControls;component/ScrollViewerStyles.xaml"/>
				<ResourceDictionary Source="/BugTracker.Screens;component/SketchStyles.xaml"/>
				<ResourceDictionary Source="/BugTracker.Screens;component/ProjectDataSources.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<Style x:Key="Layout" TargetType="{x:Type Grid}"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>